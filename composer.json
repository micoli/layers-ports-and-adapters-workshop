		"behat/mink-goutte-driver": "^1.2",
		"bunny/bunny": "^0.2.4",
		"matthiasnoback/convenient-immutability": "^0.2.1",
		"matthiasnoback/naive-serializer": "^0.4",
		"matthiasnoback/php-workshop-tools": "^0.11",
		"ramsey/uuid": "^3.4",
		"symfony/console": "^4.0",
		"symfony/debug": "^3.0",
		"symfony/debug-pack": "^1.0",
		"symfony/flex": "^1.0",
		"symfony/framework-bundle": "^4.0",
		"symfony/lts": "^4@dev",
		"symfony/psr-http-message-bridge": "^1.0",
		"symfony/web-server-bundle": "^4.0",
		"symfony/yaml": "^4.0",
		"xtreamwayz/pimple-container-interop": "^1.0",
		"zendframework/zend-expressive": "^1.0",
		"zendframework/zend-expressive-fastroute": "^1.1",
		"zendframework/zend-expressive-twigrenderer": "^1.1"
	},
	"autoload" : {
		"psr-4" : {
			"App\\" : "src/App/",
			"Meetup\\" : "src/Meetup/"
		}
